<?xml version="1.0" encoding="utf-8"?>
<com.scwang.smart.refresh.layout.SmartRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/refreshLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/rl_top"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="0dp"
                android:layout_marginRight="20dp">

                <ImageView
                    android:id="@+id/iv_avatar"
                    android:layout_width="44dp"
                    android:layout_height="44dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="0dp"
                    android:layout_marginTop="0dp"
                    android:src="@drawable/main_default_avatar"/>

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="15dp"
                    android:layout_toEndOf="@+id/iv_avatar"
                    android:gravity="center_vertical|left"
                    android:text="test"
                    android:textSize="18dp"
                    android:textStyle="bold"/>

                <View
                    android:id="@+id/v_status"
                    android:layout_width="6dp"
                    android:layout_height="6dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="41dp"
                    android:layout_toEndOf="@+id/iv_avatar"
                    android:background="@drawable/green_corner_shape"/>

                <TextView
                    android:id="@+id/tv_status"
                    android:layout_width="match_parent"
                    android:layout_height="14dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="35dp"
                    android:layout_toEndOf="@+id/v_status"
                    android:gravity="center_vertical|left"
                    android:text="@string/connected"
                    android:textColor="@color/green"
                    android:textSize="12dp"/>

                <ImageView
                    android:id="@+id/iv_add"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="20dp"
                    android:contentDescription="add"
                    android:scaleType="fitXY"
                    android:src="@drawable/main_add"
                    tools:ignore="TouchTargetSizeCheck" />

                <ImageView
                    android:id="@+id/iv_scan"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="12dp"
                    android:layout_toStartOf="@+id/iv_add"
                    android:contentDescription="scan"
                    android:scaleType="fitXY"
                    android:src="@drawable/main_scanner"
                    tools:ignore="TouchTargetSizeCheck" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_status_bg"
                android:layout_width="match_parent"
                android:layout_height="258dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="128dp"
                android:layout_marginEnd="0dp">

                <ImageView
                    android:id="@+id/iv_status_bg"
                    android:layout_width="258dp"
                    android:layout_height="258dp"
                    android:layout_centerInParent="true"
                    android:layout_marginTop="0dp"
                    android:src="@drawable/main_status_bg" />

                <TextView
                    android:id="@+id/tv_steps"
                    android:layout_width="120dp"
                    android:layout_height="38dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="57dp"
                    android:gravity="center_horizontal"
                    android:text="-"
                    android:textColor="@color/white"
                    android:textSize="32dp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/tv_step_name"
                    android:layout_width="120dp"
                    android:layout_height="14dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="96dp"
                    android:gravity="center"
                    android:text="@string/steps"
                    android:textColor="@color/dark_gray"
                    android:textSize="12dp"
                    android:textStyle="normal"/>

                <TextView
                    android:id="@+id/tv_goal"
                    android:layout_width="120dp"
                    android:layout_height="16dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="126dp"
                    android:gravity="center"
                    android:text="10000"
                    android:textColor="@color/primary_blue"
                    android:textSize="14dp"
                    android:textStyle="normal"/>

                <TextView
                    android:id="@+id/tv_goal_name"
                    android:layout_width="120dp"
                    android:layout_height="14dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="141dp"
                    android:gravity="center"
                    android:text="Goal"
                    android:textColor="@color/dark_gray"
                    android:textSize="12dp"
                    android:textStyle="normal"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_calories_bg"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="335dp"
                android:layout_marginEnd="0dp">

                <ImageView
                    android:id="@+id/iv_calories_bg"
                    android:layout_width="match_parent"
                    android:layout_height="291dp"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="20dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/main_calories_bg" />

                <TextView
                    android:id="@+id/tv_calories_value"
                    android:layout_width="120dp"
                    android:layout_height="34dp"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="18dp"
                    android:gravity="center_vertical|left"
                    android:text="-"
                    android:textSize="28dp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/tv_calories_num"
                    android:layout_width="120dp"
                    android:layout_height="16dp"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="56dp"
                    android:gravity="center_vertical|left"
                    android:text="@string/calories"
                    android:textSize="14dp"
                    android:textStyle="normal"/>

                <TextView
                    android:id="@+id/tv_distance_value"
                    android:layout_width="120dp"
                    android:layout_height="34dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="18dp"
                    android:layout_marginEnd="40dp"
                    android:gravity="center_vertical|right"
                    android:text="-"
                    android:textSize="28dp"
                    android:textStyle="bold"/>

                <TextView
                    android:id="@+id/tv_distance"
                    android:layout_width="120dp"
                    android:layout_height="16dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="56dp"

                    android:layout_marginEnd="40dp"
                    android:gravity="center_vertical|right"
                    android:text="@string/distance"
                    android:textSize="14dp"
                    android:textStyle="normal"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_behavior"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="441dp"
                android:layout_marginEnd="20dp">

                <View
                    android:id="@+id/strut"
                    android:layout_width="15dp"
                    android:layout_height="275dp"
                    android:layout_centerHorizontal="true" />

                <RelativeLayout
                    android:id="@+id/rl_heart_rate"
                    android:layout_width="0dp"
                    android:layout_height="184dp"
                    android:layout_alignEnd="@id/strut"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginEnd="15dp"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_heart_rate"
                        android:layout_width="match_parent"
                        android:layout_height="184dp"
                        android:layout_centerInParent="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_heart_rate" />

                    <ImageView
                        android:id="@+id/iv_heart_rate_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_heart_icon" />

                    <TextView
                        android:id="@+id/tv_heart_rate_name"
                        android:layout_width="120dp"
                        android:layout_height="14dp"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/heart_rate"
                        android:textColor="@color/red"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_heart_rate_num"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="59"
                        android:textColor="@color/dark_gray"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_heart_rate_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_heart_rate_num"
                        android:gravity="center_vertical|left"
                        android:text="@string/bpm"
                        android:textColor="@color/light_gray"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_sleep"
                    android:layout_width="0dp"
                    android:layout_height="76dp"
                    android:layout_alignStart="@id/strut"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="0dp"
                    android:layout_toEndOf="@+id/rl_heart_rate"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_sleep"
                        android:layout_width="match_parent"
                        android:layout_height="76dp"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="0dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_sleep_bg">

                    </ImageView>

                    <ImageView
                        android:id="@+id/iv_sleep_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_sleep_icon" />

                    <TextView
                        android:id="@+id/tv_sleep_name"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/sleep"
                        android:textColor="@color/pink"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_sleep_num"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="-"
                        android:textColor="@color/dark_gray"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_sleep_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_sleep_num"
                        android:gravity="center_vertical|left"
                        android:text="@string/hr"
                        android:textColor="@color/light_gray"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_met"
                    android:layout_width="0dp"
                    android:layout_height="76dp"
                    android:layout_alignEnd="@id/strut"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="199dp"
                    android:layout_marginEnd="15dp"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_met"
                        android:layout_width="match_parent"
                        android:layout_height="76dp"
                        android:layout_centerInParent="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_met_bg" />

                    <ImageView
                        android:id="@+id/iv_met_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_met_icon" />

                    <TextView
                        android:id="@+id/tv_met_name"
                        android:layout_width="120dp"
                        android:layout_height="14dp"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/met."
                        android:textColor="@color/orange"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_met_num"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="19"
                        android:textColor="@color/dark_gray"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_met_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_met_num"
                        android:gravity="center_vertical|left"
                        android:text="@string/met."
                        android:textColor="@color/light_gray"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_blood_pressure"
                    android:layout_width="0dp"
                    android:layout_height="184dp"
                    android:layout_alignStart="@id/strut"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="91dp"
                    android:layout_marginEnd="0dp"
                    android:layout_toEndOf="@+id/rl_heart_rate"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_blood_pressure"
                        android:layout_width="match_parent"
                        android:layout_height="184dp"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="0dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_pressure_bg">

                    </ImageView>

                    <ImageView
                        android:id="@+id/iv_blood_pressure_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/main_pressure_icon" />

                    <TextView
                        android:id="@+id/tv_pressure_name"
                        android:layout_width="120dp"
                        android:layout_height="14dp"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/blood_pressure"
                        android:textColor="@color/purple"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_pressure_value"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="-"
                        android:textColor="@color/dark_gray"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_pressure_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_pressure_value"
                        android:gravity="center_vertical|left"
                        android:text="@string/mmhg"
                        android:textColor="@color/light_gray"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_blood_oxygen"
                    android:layout_width="0dp"
                    android:layout_height="216dp"
                    android:layout_alignEnd="@id/strut"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="0dp"
                    android:layout_marginEnd="15dp"
                    android:layout_below="@+id/rl_met"
                    android:layout_marginTop="15dp"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_oxygen"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_oxygen_bg" />

                    <ImageView
                        android:id="@+id/iv_oxygen_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_oxygen" />

                    <TextView
                        android:id="@+id/tv_oxgen_name"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/oxygen"
                        android:textColor="@color/red1"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_oxygen_num"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="19"
                        android:textColor="@color/dark_gray"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_oxygen_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_oxygen_num"
                        android:gravity="center_vertical|left"
                        android:text="@string/average"
                        android:textColor="@color/light_gray"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_tempr"
                    android:layout_width="0dp"
                    android:layout_height="123dp"
                    android:layout_alignStart="@id/strut"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="15dp"
                    android:layout_alignTop="@+id/rl_blood_oxygen"
                    android:layout_marginEnd="0dp"
                    android:layout_toEndOf="@+id/rl_blood_oxygen"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_tempr"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="0dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_tempr_bg">

                    </ImageView>

                    <ImageView
                        android:id="@+id/iv_tempr_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_thermometer" />

                    <TextView
                        android:id="@+id/tv_tempr_name"
                        android:layout_width="120dp"
                        android:layout_height="14dp"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/temperature"
                        android:textColor="@color/light_blue3"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_tempr_value"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="-"
                        android:textColor="@color/dark_gray"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_tempr_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_tempr_value"
                        android:gravity="center_vertical|left"
                        android:text="@string/tempr_f"
                        android:textColor="@color/light_gray"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_weight"
                    android:layout_width="0dp"
                    android:layout_height="78dp"
                    android:layout_alignStart="@id/strut"
                    android:layout_marginStart="15dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="0dp"
                    android:layout_marginTop="15dp"
                    android:layout_below="@+id/rl_tempr"
                    tools:ignore="NotSibling">

                    <ImageView
                        android:id="@+id/iv_weight"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="0dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_weight_bg">
                    </ImageView>

                    <ImageView
                        android:id="@+id/iv_weight_icon"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="12dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/icon_weight" />

                    <TextView
                        android:id="@+id/tv_weight_name"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="36dp"
                        android:layout_marginTop="12dp"
                        android:gravity="center_vertical|left"
                        android:text="@string/weight"
                        android:textColor="@color/yellow"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_weight_value"
                        android:layout_width="wrap_content"
                        android:layout_height="24dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="15.5dp"
                        android:layout_marginBottom="16dp"
                        android:gravity="center_vertical|left"
                        android:hint="-"
                        android:textColor="@color/dark_gray"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_weight_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="6dp"
                        android:layout_marginBottom="18dp"
                        android:layout_toRightOf="@+id/tv_weight_value"
                        android:gravity="center_vertical|left"
                        android:text="@string/pounds"
                        android:textColor="@color/light_gray"
                        android:textSize="14sp"
                        android:textStyle="bold" />
                </RelativeLayout>
            </RelativeLayout>
            <LinearLayout android:layout_below="@+id/rl_behavior"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                />

        </RelativeLayout>


    </ScrollView>
</com.scwang.smart.refresh.layout.SmartRefreshLayout>